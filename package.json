{
  "name": "cyberpunk2077",
  "version": "0.2.2",
  "description": "A rebuild of the original cyberpunk vortex extension with the aim to support more mod types and fix common issues",
  "main": "./dist/index.js",
  "scripts": {
    "webpack": "node ./node_modules/webpack/bin/webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "build": "npm run webpack && extractInfo && copyfiles -f ./gameart.png ./gameicon.png ./LICENSE ./dist/",
    "pack": "npm run build && powershell.exe ./script/package.ps1",
    "pack-dev": "npm run build && powershell.exe ./script/package.ps1 -Dev",
    "pack-linux": "npm run build && bash ./script/package.sh",
    "pack-dev-linux": "npm run build && bash ./script/package.sh -dev",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --no-cache --verbose=true --silent=false",
    "test-clean": "jest --clearCache && npm run test",
    "repl": "ts-node -r tsconfig-paths/register"
  },
  "author": "KellieAtTheLab",
  "contributors": [
    "Ellie (http://üçÜüí¶üçëüò©.ws//)",
    "Auska (https://github.com/effs)",
    "ThunderGod (https://github.com/thundergod97)"
  ],
  "license": "GPL-3.0",
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.20.46",
    "@types/react": "^16.9.43",
    "@types/react-bootstrap": "^0.32.20",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "bluebird": "^3.7.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.0",
    "i18next": "^19.0.1",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^12.3.4",
    "mock-fs": "^5.1.2",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "react": "^16.12.0",
    "react-bootstrap": "^0.33.0",
    "react-dom": "^16.12.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^6.0.4",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^3.5.2",
    "vortex-api": "github:Nexus-Mods/vortex-api",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "key-tree": "^1.0.3"
  }
}
